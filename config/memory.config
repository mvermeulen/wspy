#
# Measure memory
command --counterinfo /home/mev/.wspy/counters
command --perfcounters
command --perfcounter-model core
command --set-counters instructions,cpu-cycles
command --set-counters 0:data_writes,data_reads
command --set-counters 1:PERF_COUNT_HW_CACHE_LL_WR_MISS,PERF_COUNT_HW_CACHE_LL_WR_ACCESS
command --set-counters 2:PERF_COUNT_HW_CACHE_LL_RD_MISS,PERF_COUNT_HW_CACHE_LL_RD_ACCESS
